SELECT
  EXTRACT(MONTH FROM tr.VERIFIED_DT) AS VERIFIED_MONTH,
  tr.TEST_PERFORMING_LOCATION AS DEPOT,
  ot.ORDER_NO AS ACCESSION
FROM V_P_LAB_TEST_RESULT tr
  INNER JOIN V_P_LAB_ORDERED_TEST ot
    ON ot.ORDER_AA_ID = tr.ORDER_AA_ID
       AND ot.TEST_ID = tr.GROUP_TEST_ID
       AND ot.WORKSTATION_ID = tr.ORDERING_WORKSTATION_ID
WHERE
  tr.STATE IN ('Final', 'Corrected')
  AND tr.GROUP_TEST_ID = :TEST_ID
  AND tr.VERIFIED_DT IS NOT NULL
  AND ot.CANCELLED_FLAG = 0
  AND tr.RESULT <> '.'
  AND tr.VERIFIED_DT >= ADD_MONTHS(TRUNC(SYSDATE, 'MM'), -1)
ORDER BY VERIFIED_MONTH